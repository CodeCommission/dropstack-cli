#!/usr/bin/env node
const BASE_URL = `https://api.dropstack.run`;

const fs = require('fs');
const path = require('path');
const program = require('commander');
const fetch = require('node-fetch');
const stackSettings = require('../lib/stack-settings')();

program
  .arguments('<id> <url>')
  .action(alias)
  .parse(process.argv);

function alias(id, url){
  console.log(`Alias for ${id} ${url}`);
  stackSettings.load()
  .then(console.log)
  .catch(console.error);
}
